{
  "extends": [
    "config:base"
  ],
  "dockerfile": {
    "enabled": false
  },
  "pip-compile": {
    "enabled": true,
    "includePaths":["batch/aiml-workloads/**"],
    "fileMatch": ["(^|/)requirements\\.in$"],
    "lockFileMaintenance": {
      "enabled": true
    }
  },
  "prConcurrentLimit":5,
  "stabilityDays":7,
  "vulnerabilityAlerts":{
     "labels":[
       "type:security" 
     ],
     "stabilityDays":0
  },
  "labels": ["dependencies"],
  "python":{
    "addLabels": ["lang: python"]
  },
  "java":{
    "addLabels": ["lang: java"]
  },
  "golang":{
    "addLabels": ["lang: go"]
  },
  "nuget":{
    "addLabels": ["lang: dotnet"]
  }
}